{
  "name": "api-nodewebpack",
  "version": "1.2.0",
  "description": "node webpack api",
  "main": "./dist/server.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "pm2": "pm2 start ./dist/server.js",
    "postinstall": "npm run build",
    "test": "jest --forceExit",
    "test-ci": "npm test && cat ./coverage/lcov.info | coveralls",
    "start": "  node server.js",
    "production": " node server.js",
    "qa": " npm run build && node ./dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Jaspal Singh",
  "license": "MIT",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "server"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/tests/**/*.test.(ts|js)"
    ],
    "testEnvironment": "node",
    "mapCoverage": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/coverage/**",
      "!**/webpack.config.js"
    ]
  },
  "engines": {
    "node": "9.2.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.4",
    "@types/chai": "^4.0.1",
    "@types/dotenv": "^4.0.0",
    "@types/express": "^4.0.36",
    "@types/jest": "^20.0.4",
    "@types/joi": "^13.0.5",
    "@types/nconf": "0.0.34",
    "@types/node": "^8.0.13",
    "@types/pg": "^7.4.8",
    "@types/sequelize": "^4.27.16",
    "@types/supertest": "^2.0.2",
    "@types/winston": "^2.3.9",
    "chai": "^4.1.0",
    "coveralls": "^3.0.0",
    "jest": "^21.2.1",
    "mocha": "^4.0.1",
    "supertest": "^3.0.0",
    "ts-jest": "^21.2.4",
    "ts-loader": "^3.2.0",
    "ts-node": "^3.2.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "webpack": "^3.9.1"
  },
  "dependencies": {
    "bcrypt": "^2.0.1",
    "body-parser": "^1.17.2",
    "chai-http": "^4.0.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "joi": "^13.1.2",
    "jsonwebtoken": "^8.2.1",
    "morgan": "^1.9.0",
    "mysql2": "^1.5.3",
    "nconf": "^0.8.5",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "pm2": "^2.7.1",
    "sequelize": "^4.37.6",
    "sqlite3": "^4.0.0",
    "tedious": "^2.3.1",
    "webpack-cli": "^2.1.2",
    "winston": "^2.4.2",
    "xlsx-to-json": "^0.3.0"
  }
}
